<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	version="2.5">
	<display-name>servicioRestFulAlumnos</display-name>
	<servlet>
		<description>JAX-RS Tools Generated - Do not modify</description>
		<servlet-name>AlumnosRestFulService</servlet-name>
		<servlet-class>com.sun.jersey.spi.container.servlet.ServletContainer</servlet-class>
		<init-param>
			<param-name>com.sun.jersey.config.property.packages</param-name>
			<param-value>es.domingojunta.rest.services</param-value>
		</init-param>
		<init-param>
			<param-name>con.sun.jersey.api.json.POJOMappingFeature</param-name>
			<param-value>true</param-value>
		</init-param>
		<load-on-startup>1</load-on-startup>
	</servlet>
	<servlet-mapping>
		<servlet-name>AlumnosRestFulService</servlet-name>
		<url-pattern>/api-rest/*</url-pattern>
	</servlet-mapping>
</web-app>